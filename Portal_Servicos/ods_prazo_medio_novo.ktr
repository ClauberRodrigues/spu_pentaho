    <connection>PainelProd</connection>
    <sql>select vods.uf, vods.servico, vods.situacao,
       coalesce(sum(vods.nr_dias_decorridos) / count(distinct vods.id_requerimento),0.00) nr_prazo_medio
  from (select ods.id_requerimento,
               ods.ds_uf uf,
               ods.ds_sigla servico,
       ods.ds_situacao_atual situacao,
       case when ods.cd_posicao_consolidada = 2 then
            sum(ods.pz_dias_conclusao) -
                (select coalesce(sum(ods2.pz_dias_conclusao),0)
                   from biportal.ods_apura_prazo_medio ods2
                  where ods2.id_requerimento = ods.id_requerimento
                    and ods2.ds_uf = ods.ds_uf
                    and ods2.ds_sigla = ods.ds_sigla
                    and ods2.ds_situacao_atual = 'Aguardando Requerente'
                  group by ods2.ds_uf, ods2.ds_sigla, ods2.ds_situacao_atual, ods2.id_requerimento) 
       else sum(ods.pz_dias_conclusao) end nr_dias_decorridos
  from biportal.ods_apura_prazo_medio ods
 where ods.ds_situacao_atual &lt;&gt; 'Aguardando Requerente'
group by ods.ds_uf, ods.ds_sigla, ods.ds_situacao_atual, ods.id_requerimento, ods.cd_posicao_consolidada) vods
group by vods.uf, vods.servico, vods.situacao
order by vods.uf, vods.servico, vods.situacao</sql>
