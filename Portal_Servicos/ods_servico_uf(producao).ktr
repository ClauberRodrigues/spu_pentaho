    <connection>SISREI</connection>
    <sql>select distinct&#xa;       cp.sg_estadosuperintendencia ds_uf,&#xa;       cp.id_consultaprevia as id_requerimento,&#xa;       99 as id_servico,&#xa;       &#x27;Req. Imovel&#x2f;SISREI&#x27; as ds_sigla,&#xa;       cp.dt_criacao&#x3a;&#x3a;timestamp as dt_abertura_requerimento,&#xa;       mcp.dt_mudancasituacao&#x3a;&#x3a;timestamp dt_alteracao,&#xa;       --scp.ds_situacaoconsultaprevia,&#xa;       CASE&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Solicitada&#x27; then &#x27;Aguardando Triagem&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Aguardando maiores informa&#xe7;&#xf5;es&#x27; then &#x27;Aguardando Requerente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aguardando aprova&#xe7;&#xe3;o&#x27; then &#x27;Aguardando Triagem&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aprovada&#x27; then &#x27;Aguardando Requerente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Requerimento solicitado&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Ofertas rejeitadas pelo requerente&#x27; then &#x27;Aguardando Triagem&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Rejei&#xe7;&#xe3;o das ofertas aprovada pelo superintendente&#x27; then &#x27;Indeferido&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Cancelada&#x27; then &#x27;Cancelado&#x27;&#x3a;&#x3a;text &#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Indeferido&#x27; then &#x27;Indeferido&#x27;&#x3a;&#x3a;text &#xa;            ELSE &#x27;Novo&#x27;&#x3a;&#x3a;text&#xa;       END AS ds_situacao_atual,&#xa;       CASE&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Solicitada&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Aguardando maiores informa&#xe7;&#xf5;es&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aguardando aprova&#xe7;&#xe3;o&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aprovada&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Requerimento solicitado&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Ofertas rejeitadas pelo requerente&#x27; then 1&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Rejei&#xe7;&#xe3;o das ofertas aprovada pelo superintendente&#x27; then 2&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Cancelada&#x27; then 2 &#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Indeferido&#x27; then 2 &#xa;            ELSE 1&#xa;       END AS cd_posicao_consolidada,&#xa;       CASE&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Solicitada&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Aguardando maiores informa&#xe7;&#xf5;es&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aguardando aprova&#xe7;&#xe3;o&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Avalia&#xe7;&#xe3;o aprovada&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Requerimento solicitado&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Ofertas rejeitadas pelo requerente&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Rejei&#xe7;&#xe3;o das ofertas aprovada pelo superintendente&#x27; then &#x27;Conclu&#xed;do&#x27;&#x3a;&#x3a;text&#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Cancelada&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text &#xa;            WHEN scp.ds_situacaoconsultaprevia &#x3d; &#x27;Indeferido&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text &#xa;            ELSE &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;       END AS ds_posicao_consolidada,&#xa;       CASE&#xa;            WHEN scp.ds_situacaoconsultaprevia&#x3a;&#x3a;text &#x3d; &#x27;Cancelada&#x27; &#xa;            THEN mcp.dt_mudancasituacao&#x3a;&#x3a;date - cp.dt_criacao&#x3a;&#x3a;date&#xa;            ELSE -1&#x3a;&#x3a;integer  --sem prazo&#xa;        END AS pz_dias_conclusao,&#xa;        1 AS contador&#xa;  from sisrei.sisrei.tb_consultaprevia cp&#xa;       inner join sisrei.sisrei.tb_mudancasituacaoconsultaprevia mcp&#xa;          on mcp.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;       inner join sisrei.sisrei.tb_situacaoconsultaprevia scp&#xa;          on scp.id_situacaoconsultaprevia &#x3d; mcp.co_situacaoposterior&#xa;       left  join sisrei.sisrei.tb_requerimento rq &#xa;             inner join sisrei.sisrei.tb_mudancasituacaorequerimento mrq&#xa;                   inner join sisrei.sisrei.tb_situacaorequerimento srq &#xa;                           on srq.id_situacaorequerimento &#x3d; mrq.co_situacaoposterior&#xa;                   on mrq.co_requerimento &#x3d; rq.id_requerimento&#xa;          on rq.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;union&#xa;select distinct&#xa;       cp.sg_estadosuperintendencia ds_uf,&#xa;       cp.id_consultaprevia as id_requerimento,&#xa;       99 as id_servico,&#xa;       &#x27;Req. Imovel&#x2f;SISREI&#x27; as ds_sigla,&#xa;       cp.dt_criacao&#x3a;&#x3a;timestamp as dt_abertura_requerimento,&#xa;       &#x28;cp.dt_ultimaalteracaosituacao&#x3a;&#x3a;date &#x2b;1&#x29;&#x3a;&#x3a;timestamp dt_alteracao,&#xa;       --scp.ds_situacaoconsultaprevia,&#xa;       &#x27;Indeferido&#x27; as ds_situacao_atual,&#xa;       2 cd_posicao_consolidada,&#xa;       &#x27;Cancelada&#x27; ds_posicao_consolidada,&#xa;       &#x28;cp.dt_ultimaalteracaosituacao&#x3a;&#x3a;date &#x2b;1&#x29; - cp.dt_criacao&#x3a;&#x3a;date pz_dias_conclusao,&#xa;        1 AS contador&#xa;  from sisrei.sisrei.tb_consultaprevia cp&#xa;       inner join sisrei.sisrei.tb_mudancasituacaoconsultaprevia mcp&#xa;          on mcp.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;       inner join sisrei.sisrei.tb_situacaoconsultaprevia scp&#xa;          on scp.id_situacaoconsultaprevia &#x3d; mcp.co_situacaoposterior&#xa;       left  join sisrei.sisrei.tb_requerimento rq &#xa;             inner join sisrei.sisrei.tb_mudancasituacaorequerimento mrq&#xa;                   inner join sisrei.sisrei.tb_situacaorequerimento srq &#xa;                           on srq.id_situacaorequerimento &#x3d; mrq.co_situacaoposterior&#xa;                   on mrq.co_requerimento &#x3d; rq.id_requerimento&#xa;          on rq.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;where cp.co_situacaoconsultaprevia &#x3d; 4&#xa;  and co_tipoavaliacao &#x3d; 2&#xa;  and ic_aprovadapelosuperintendente is true&#xa;union&#xa;select distinct &#xa;       cp.sg_estadosuperintendencia ds_uf,&#xa;       --rq.id_requerimento as id_requerimento,&#xa;       cp.id_consultaprevia as id_requerimento,&#xa;       99 as id_servico,&#xa;       &#x27;Req. Imovel&#x2f;SISREI&#x27; as ds_sigla,  &#xa;       --rq.dt_criacao&#x3a;&#x3a;timestamp as dt_abertura_requerimento,&#xa;       cp.dt_criacao&#x3a;&#x3a;timestamp as dt_abertura_requerimento,&#xa;       mrq.dt_mudancasituacao&#x3a;&#x3a;timestamp dt_alteracao,&#xa;       --srq.ds_situacaorequerimento &#xa;       CASE&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicita&#xe7;&#xe3;o criada&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicitado&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Avaliado&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aprovado&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Processo administrativo aberto&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Indeferido&#x27; then &#x27;Indeferido&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o n&#xe3;o autorizada&#x27; then &#x27;Indeferido&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o da portaria&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o autorizada&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Minuta de contrato&#x2f;termo enviado&#x27; then &#x27;Aguardando Requerente&#x27;&#x3a;&#x3a;text &#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o do extrato&#x27; then &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o conclu&#xed;da&#x27; then &#x27;Deferido&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Cancelado&#x27; then &#x27;Cancelado&#x27;&#x3a;&#x3a;text &#xa;            ELSE &#x27;Novo&#x27;&#x3a;&#x3a;text&#xa;       END AS ds_situacao_atual,&#xa;       CASE&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicita&#xe7;&#xe3;o criada&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicitado&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Avaliado&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aprovado&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Processo administrativo aberto&#x27; then 1&#xa;            when srq.ds_situacaorequerimento &#x3d; &#x27;Indeferido&#x27; then 2&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o n&#xe3;o autorizada&#x27; then 2&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o da portaria&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o autorizada&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Minuta de contrato&#x2f;termo enviado&#x27; then 1 &#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o do extrato&#x27; then 1&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o conclu&#xed;da&#x27; then 2&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Cancelado&#x27; then 2 &#xa;            ELSE 1&#xa;       END AS cd_posicao_consolidada,       &#xa;       CASE&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicita&#xe7;&#xe3;o criada&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Solicitado&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Avaliado&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aprovado&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Processo administrativo aberto&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Indeferido&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o n&#xe3;o autorizada&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o da portaria&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o autorizada&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Minuta de contrato&#x2f;termo enviado&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Aguardando publica&#xe7;&#xe3;o do extrato&#x27; then &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Destina&#xe7;&#xe3;o conclu&#xed;da&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN srq.ds_situacaorequerimento &#x3d; &#x27;Cancelado&#x27; then &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text &#xa;            ELSE &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;       END AS ds_posicao_consolidada,&#xa;       CASE&#xa;            WHEN srq.ds_situacaorequerimento&#x3a;&#x3a;text &#x3d; ANY &#x28;ARRAY&#x5b;&#x27;Cancelado&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;Destina&#xe7;&#xe3;o conclu&#xed;da&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;Destina&#xe7;&#xe3;o n&#xe3;o autorizada&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;Indeferido&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text&#x5d;&#x29; &#xa;            THEN mrq.dt_mudancasituacao&#x3a;&#x3a;date - rq.dt_criacao&#x3a;&#x3a;date&#xa;            ELSE -1&#x3a;&#x3a;integer  --sem prazo&#xa;        END AS pz_dias_conclusao,&#xa;    1 AS contador&#xa;  from sisrei.sisrei.tb_consultaprevia cp&#xa;       inner join sisrei.sisrei.tb_mudancasituacaoconsultaprevia mcp&#xa;          on mcp.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;       inner join sisrei.sisrei.tb_situacaoconsultaprevia scp&#xa;          on scp.id_situacaoconsultaprevia &#x3d; mcp.co_situacaoposterior&#xa;       INNER  join sisrei.sisrei.tb_requerimento rq &#xa;             inner join sisrei.sisrei.tb_mudancasituacaorequerimento mrq&#xa;                   inner join sisrei.sisrei.tb_situacaorequerimento srq &#xa;                           on srq.id_situacaorequerimento &#x3d; mrq.co_situacaoposterior&#xa;                   on mrq.co_requerimento &#x3d; rq.id_requerimento&#xa;          on rq.co_consultaprevia &#x3d; cp.id_consultaprevia&#xa;order by dt_alteracao</sql>
