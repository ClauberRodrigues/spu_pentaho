    <sql>SELECT&#xa;        CASE&#xa;            WHEN ei.ds_uf IS NOT NULL THEN ei.ds_uf&#x3a;&#x3a;bpchar&#xa;            WHEN er.ds_uf IS NOT NULL THEN er.ds_uf&#x3a;&#x3a;bpchar&#xa;            ELSE &#x27;NI&#x27;&#x3a;&#x3a;character&#x28;2&#x29;&#xa;        END AS ds_uf,&#xa;    r.id_requerimento,&#xa;    sv.id_servico,&#xa;    substring&#x28;sv.ds_titulo,1,20&#x29; ds_sigla,&#xd;&#xa;    r.dt_envio&#x3a;&#x3a;timestamp AS dt_abertura_requerimento,&#xa;    coalesce&#x28;hr.dt_alteracao&#x3a;&#x3a;timestamp,r.dt_envio&#x3a;&#x3a;timestamp&#x29; AS dt_alteracao,&#xa;        CASE &#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE&#x27;&#x3a;&#x3a;text THEN &#x27;Aguardando Triagem&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_TECNICA&#x27;&#x3a;&#x3a;text THEN &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_PREVIA&#x27;&#x3a;&#x3a;text THEN &#x27;Em An&#xe1;lise Pr&#xe9;via&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE_PREVIA&#x27; THEN &#x27;Aguardando Triagem&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;COM_PENDENCIA&#x27;&#x3a;&#x3a;text THEN &#x27;Aguardando Requerente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_REQUERENTE&#x27;&#x3a;&#x3a;text THEN &#x27;Aguardando Requerente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;PROCESSO_ABERTO&#x27;&#x3a;&#x3a;text THEN &#x27;Em An&#xe1;lise T&#xe9;cnica&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;DEFERIDO&#x27;&#x3a;&#x3a;text THEN &#x27;Deferido&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;INDEFERIDO&#x27;&#x3a;&#x3a;text THEN &#x27;Indeferido&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;CANCELADO&#x27;&#x3a;&#x3a;text THEN &#x27;Cancelado&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EXPIRADO&#x27;&#x3a;&#x3a;text THEN &#x27;Cancelado&#x27;&#x3a;&#x3a;text&#xa;            ELSE r.ds_status&#x3a;&#x3a;text&#xa;        END AS ds_situacao_atual,&#xa;&#xa;        CASE&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE_PREVIA&#x27; THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_TECNICA&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_PREVIA&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;COM_PENDENCIA&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_REQUERENTE&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;PROCESSO_ABERTO&#x27;&#x3a;&#x3a;text THEN 1&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;DEFERIDO&#x27;&#x3a;&#x3a;text THEN 2&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;INDEFERIDO&#x27;&#x3a;&#x3a;text THEN 2&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;CANCELADO&#x27;&#x3a;&#x3a;text THEN 2&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EXPIRADO&#x27;&#x3a;&#x3a;text THEN 2&#xa;            ELSE 1&#xa;        END AS cd_posicao_consolidada,&#xa;        CASE&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE&#x27;&#x3a;&#x3a;text THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;AGUARDANDO_ANALISE_PREVIA&#x27; THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_TECNICA&#x27;&#x3a;&#x3a;text THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EM_ANALISE_PREVIA&#x27;&#x3a;&#x3a;text THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;COM_PENDENCIA&#x27;&#x3a;&#x3a;text THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;PROCESSO_ABERTO&#x27;&#x3a;&#x3a;text THEN &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;DEFERIDO&#x27;&#x3a;&#x3a;text THEN &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;INDEFERIDO&#x27;&#x3a;&#x3a;text THEN &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;CANCELADO&#x27;&#x3a;&#x3a;text THEN &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; &#x27;EXPIRADO&#x27;&#x3a;&#x3a;text THEN &#x27;Conclu&#xed;do&#x200b;&#x27;&#x3a;&#x3a;text&#xa;            ELSE &#x27;Pendente&#x27;&#x3a;&#x3a;text&#xa;        END AS ds_posicao_consolidada,&#xa;&#xa;        CASE&#xa;            WHEN r.ds_status&#x3a;&#x3a;text &#x3d; ANY &#x28;ARRAY&#x5b;&#x27;DEFERIDO&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;INDEFERIDO&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;CANCELADO&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text, &#x27;EXPIRADO&#x27;&#x3a;&#x3a;character varying&#x3a;&#x3a;text&#x5d;&#x29; &#xa;            THEN coalesce&#x28;hr.dt_alteracao&#x3a;&#x3a;date,r.dt_envio&#x3a;&#x3a;date&#x29; - r.dt_envio&#x3a;&#x3a;date&#xa;            ELSE coalesce&#x28;hr.dt_alteracao&#x3a;&#x3a;date,r.dt_envio&#x3a;&#x3a;date&#x29; - r.dt_envio&#x3a;&#x3a;date -- -1&#x3a;&#x3a;integer  --sem prazo&#xa;        END AS pz_dias_conclusao,&#xa;        --r.id_procedimento_sei,&#xa;    1 AS contador&#xa;   FROM servico.tb_requerimento r&#xa;        LEFT  JOIN servico.tb_hist_requerimento hr ON hr.id_requerimento &#x3d; r.id_requerimento&#xa;        LEFT  JOIN &#x28;servico.tb_imovel i INNER JOIN servico.tb_endereco ei ON ei.id_endereco &#x3d; i.id_endereco&#x29; ON i.id_imovel &#x3d; r.id_imovel&#xa;        LEFT  JOIN &#x28;servico.tb_requerente rqe &#xa;                   LEFT JOIN &#x28;servico.tb_evento ev INNER JOIN servico.tb_endereco er ON er.id_endereco &#x3d; ev.id_endereco&#x29; &#xa;                          ON rqe.id_evento &#x3d; ev.id_evento&#x29; &#xa;               ON rqe.id_requerimento &#x3d; r.id_requerimento&#xa;        LEFT  JOIN servico.tb_servico sv on sv.id_servico &#x3d; r.id_servico and ds_link is null&#x3b;</sql>
