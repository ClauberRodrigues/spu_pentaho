    <connection>siapa</connection>
    <sql>select distinct c.nu_credito,
       d.nu_debito nu_debito_parc,
       case when cob.va_total_deb_cobr is not null then cob.va_total_deb_cobr
            when fcl.va_laudemio_total is not null then fcl.va_laudemio_total
            when d.va_debito_total is not null then d.va_debito_total
            else c.va_total_cred end va_total_cred,
       a.da_arrecadacao da_arrecadacao_cred,
       c.co_tipo_credito,
       a.va_total_documento va_tributo_darf_1,
       0.00 va_tributo_darf_2,
       0.00 va_tributo_darf_3,
       0.00 va_tributo_darf_4,
       0.00 va_tributo_darf_5,
       case when d.co_receita is not null then d.co_receita
            when cob.co_receita_deb_cobr is not null then cob.co_receita_deb_cobr
            when fcl.nu_fcl is not null then '2081' 
            else a.co_receita_principal end co_tributo_darf_1,
       '0000' co_tributo_darf_2,
       '0000' co_tributo_darf_3,
       '0000' co_tributo_darf_4,
       '0000' co_tributo_darf_5,
       c.co_situacao_cred, sc.no_situacao_cred, 
       c.nu_rip, a.nu_darf_senda cred_rip_utilizacao, 
       m.sg_uf, c.nu_pagamento_darf,
       c.nu_basico_cpf_cgc_cred as nu_cpf_cgc,
       c.nu_cpf_cgc_darf,
       nu_cobranca_cred,
       'darf multiplo' as fonte
  from siapa.siapa_a_darf_arrecadacao a 
       left join siapa.siapa_a_credito c 
                 left join siapa.v_situacao_credito sc on sc.co_situacao_cred = c.co_situacao_cred
              on c.nu_darf_senda = a.nu_darf_senda 
       left join siapa.siapa_a_darf_emissao em  
            left join siapa.siapa_a_debito d on lpad(cast(em.nu_identificador_origem as bigint)::varchar(8),8,'0') = d.nu_debito 
                                                and substring(em.sg_identificador_origem,1,3) = 'DEB'
            left join siapa.siapa_a_fcl_nova fcl on fcl.nu_fcl = substring(em.nu_identificador_origem,8,8)
                                                and substring(em.sg_identificador_origem,1,3) = 'FCL'
            left join siapa.siapa_a_cobranca cob on cob.nu_cobranca_lista = substring(em.nu_identificador_origem,10,8) 
                                                 and cob.co_receita_deb_cobr in ('2090','2073','2081')
                                                 and substring(em.sg_identificador_origem,1,3) = 'COB'
            left join siapa.siapa_a_imovel i  on i.nu_rip = (case when fcl.nu_rip is not null then fcl.nu_rip
                                                                  when cob.nu_rip_cobr is not null then cob.nu_rip_cobr
                                                                  else d.nu_rip end)
       on em.nu_darf_senda = a.nu_darf_senda          
       left join siapa.v_municipio m on m.nu_mun = (case when i.ed_mun is not null then i.ed_mun 
                                                          when c.nu_mun_arrec_darf is not null then c.nu_mun_arrec_darf
                                                          else a.nu_municipio_arrecadacao end)     
 where rtrim(a.co_receita_principal) in ('4444')
   and ltrim(rtrim(sg_identificador_origem)) = 'COB'  
</sql>
