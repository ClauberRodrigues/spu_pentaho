       left  join siapa.siapa_a_credito cd on cd.nu_darf_senda = a.nu_darf_senda
       left  join (select cob.nu_cobranca_lista, cob.co_receita_deb_cobr co_receita, sum(cob.va_principal_deb_cobr) va_receita
