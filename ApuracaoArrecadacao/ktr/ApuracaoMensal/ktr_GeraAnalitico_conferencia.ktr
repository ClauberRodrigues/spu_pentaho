-- Arrecadacao CIF
union
select 'CIF'::varchar(7) origem, '0110' nu_dpu, 'DF' ed_sg_uf_dpu, 
        NULL nu_darf_senda, 
        a.cd_receita co_receita, a.ds_receita no_receita,
        (a.nu_ano||'-'||a.nu_mes||'-'||'01'::char(02))::date dt_arrecadacao, a.vl_arrecadado vl_receita, null id_imovel,
       a.nu_ano,
       a.nu_mes
  from arrecadacao.tb_arrecadacao_cif a 
 where a.nu_ano::integer = ${Ano}
   and a.nu_mes::integer = ${Mes}  
---------------------------------------------------------------------------------------------------------------
-- Arrecadacao Parcelamento (SPUNet)
union
select 'SPUNET'::varchar(7) origem, taps.nu_superintendencia nu_dpu, 
       case when m.sg_uf is null then taps.sg_uf else m.sg_uf end ed_sg_uf_dpu, taps.nu_documentorfb nu_darf_senda, taps.co_receita co_receita, 
       rc.no_receita no_receita,
       taps.dt_arrecadacao da_arrecadacao, taps.vl_arrecadado vl_receita, taps.nu_parcelamento id_imovel, 
       extract(year from taps.dt_arrecadacao)::varchar(4) nu_ano, LPAD(extract(month from taps.dt_arrecadacao)::varchar(2),2,'0') nu_mes
  from arrecadacao.tb_arrecadacao_parcelamento_spunet taps
       left join oltp.siapa.v_receita rc on rc.co_receita  = taps.co_receita 
       left join oltp.siapa.v_municipio m on m.nu_mun = taps.nu_municipio
 where extract(year from taps.dt_arrecadacao)::varchar(4) = ${Ano}
   and LPAD(extract(month from taps.dt_arrecadacao)::varchar(2),2,'0') = ${Mes} 
 order by 2, 4, 6;
