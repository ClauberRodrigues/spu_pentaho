    <sql>-- ANALÍTICO SIAPA GERAL
select 'SIAPA'::varchar(7) origem, e.nu_dpu, dpu.ed_sg_uf_dpu, a.nu_darf_senda, a.co_receita_principal::varchar(4) co_receita, dr.no_receita,
       da_arrecadacao, a.va_principal vl_receita, cd.nu_rip::varchar(20) id_imovel,
       extract(year from da_arrecadacao)::varchar(4) nu_ano,
       extract(month from da_arrecadacao)::varchar(2) nu_mes
  from siapa.siapa_a_darf_arrecadacao a 
       inner join siapa.siapa_a_darf_emissao e on e.nu_darf_senda = a.nu_darf_senda
       left  join siapa.siapa_a_credito cd on cd.nu_darf_senda = a.nu_darf_senda
       left  join siapa.v_receita dr on dr.co_receita = a.co_receita_principal
       left  join siapa.v_dpu dpu on dpu.nu_dpu = e.nu_dpu
 where extract(year from da_arrecadacao)  = ${Ano}
   and extract(month from da_arrecadacao) = ${Mes}
   and a.co_receita_principal NOT IN ('4444','2049')
   and a.co_sistema_origem = '001'
union
-------------------------------------------------------------------------------------------------------------------------------------
-- ANALÍTICO SIAPA GERAL - MULTA
select 'SIAPA' origem, e.nu_dpu, dpu.ed_sg_uf_dpu, a.nu_darf_senda,
       a.co_receita_multa::varchar(4) co_receita, dr.no_receita,
       da_arrecadacao, a.va_multa vl_arrecadado, cd.nu_rip::varchar(20) id_imovel,
       extract(year from da_arrecadacao)::varchar(4) nu_ano,
       extract(month from da_arrecadacao)::varchar(2) nu_mes
  from siapa.siapa_a_darf_arrecadacao a 
       inner join siapa.siapa_a_darf_emissao e on e.nu_darf_senda = a.nu_darf_senda
       left  join siapa.siapa_a_credito cd on cd.nu_darf_senda = a.nu_darf_senda
       left  join siapa.v_receita dr on dr.co_receita = a.co_receita_multa
       left  join siapa.v_dpu dpu on dpu.nu_dpu = e.nu_dpu
 where extract(year from da_arrecadacao)=${Ano}    
   and extract(month from da_arrecadacao)=${Mes}
   and a.co_receita_principal &lt;&gt; '4444'
   and a.va_multa > 0.00 
   and a.co_sistema_origem = '001'
