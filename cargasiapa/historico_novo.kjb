      <script>#!/bin/bash
ARQUIVO=${ARQUIVO}
PASTA=${PASTA}
COMMAND=7za;
OPTIONS="x ";

command -v $COMMAND > /dev/null &amp;&amp; EXIST=0 || EXIST=1;
if [ $EXIST -eq 1 ]; then
        echo 'Comando 7zr não existe instale p7zip-rar e p7zip';
fi

#echo "Teste Clauber " $(find $PASTA$ARQUIVO*.7z);

for ARQ in $(find $PASTA$ARQUIVO*.7z); do
        #echo "Teste descomp " $COMMAND $OPTIONS "$ARQ";
        ARQDESCOMPACTADO=`ls $ARQ`
        ARQDESCOMPACTADO=`echo ${ARQDESCOMPACTADO/$PASTA/}` # Pega o nome correto do arquivo com a extencao DYYMM10 do SIAPA
        ARQDESCOMPACTADO=`echo ${ARQDESCOMPACTADO%.*}` # Remove a extencao do arquivo 7z

        if [ ! -e $ARQ ]; then
                exit 1;
        fi

        if [ -d $PASTA ]; then
                cd $PASTA;
        else
                echo "ERROR: nao eh pasta";
                exit 1
        fi
        # Se existe o arquivo descompactado nao faca nada.


       # if [ ! -e $PASTA/$ARQDESCOMPACTADO ]; then
                $COMMAND $OPTIONS "$ARQ";
        #fi


done

exit 0</script>
