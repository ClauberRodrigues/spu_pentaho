      <description/>
      <type>EVAL</type>
      <script>var today = new Date();
var dd = today.getDate();
var mm = today.getMonth()+1; //January is 0!
var yyyy = today.getFullYear()
var yy = today.getFullYear().toString().slice(2);
if(mm&lt;10){mm='0'+mm} ;

int_d = new Date(yyyy, mm,1);
last_d = new Date(int_d - 1);
last_d = last_d.getDate();

DATE_INICIO=yyyy+'-'+mm+'-01'
DATE_FIM=yyyy+'-'+mm+'-'+last_d

parent_job.setVariable("DATA_INICIO_MES", DATE_INICIO);
parent_job.setVariable("DATA_FIM_MES", DATE_FIM);
true;</script>
