      <script>#!/bin/bash
ARQUIVO=${ARQUIVO}
PASTA=${PASTA}
COMMAND=7za;
OPTIONS="x  -y ";

command -v $COMMAND > /dev/null &amp;&amp; EXIST=0 || EXIST=1;

if [ $EXIST -eq 1 ]; then
       echo 'Comando 7za não existe instale p7zip-rar e p7zip';
      exit 1;
fi

if [ -d $PASTA ]; then
      cd $PASTA;
else
      echo "ERROR: nao eh pasta";
       exit 1
fi

$COMMAND $OPTIONS  "$ARQUIVO";

exit 0</script>
