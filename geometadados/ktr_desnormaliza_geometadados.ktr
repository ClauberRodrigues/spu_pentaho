    <sql>select ct.id_produto_cartografico,
       ct.ic_situacao_demarcacao,
       ct.ds_justificativa_demarcacao,
       ct.no_titulo,
       ct.ds_resumo,
       ct.ds_informacoes_adicionais_identificacao,
       ct.co_status_produto, sp.no_status_produto,
       ct.co_tipo_produto, tp.no_tipo_produto,
       ct.co_responsavel_identificacao, rpi.no_instituicao,
       ct.co_funcao_identificacao, fi.no_funcao no_funcao_identificacao,
       ct.co_sistema_referencia, sr.no_elipsoide, sr.no_origem, sr.no_sistema_referencia,
       ct.co_projecao, pj.no_projecao, pj.nu_srid_epsg,
       ct.vl_resolucao, geocodigo_municipio_identificacao_cdg, ct.co_datum,
       ct.vl_minimo, ct.vl_maximo, 
       ct.co_representacao_espacial, re.no_representacao_espacial,
       ct.co_escala, ec.vl_escala,
       ct.co_idioma_id_cdg, idm.no_idioma,
       ct.co_encode_id_cdg, ecid.no_encode,
       ct.co_categoria_tematica, ctm.no_categoria_tematica,
       ct.ds_linhagem,
       ct.co_nivel_hierarquico, nh.no_nivel_hierarquico,
       ct.ds_vinculo,
       ct.co_acesso_off, off.no_acesso_off_line,
       ct.ds_informacoes_adicionais_distribuicao, 
       ct.co_formato_distribuicao, fdt.no_formato_distribuicao,
       ct.co_protocolos, ptc.no_protocolo,
       ct.co_responsavel_distribuicao, rpd.no_instituicao no_instituicao_distribuicao,
       ct.co_funcao_distribuicao, fd.no_funcao no_funcao_distribuicao,
       ct.co_idioma_metadado, idmtd.no_idioma no_idioma_metadados,
       ct.co_encode_metadado, ecidmd.no_encode no_encode_metadado,
       ct.co_norma_designacao, ndes.no_norma_designacao,
       ct.co_responsavel_metadados, rpm.no_instituicao no_intituicao_metadados,
       ct.co_funcao_metadados, fim.no_funcao no_funcao_metadados,
       ct.ic_ponto_controle, ct.ic_identificador_unico_imovel,
       ct.co_tipo_de_articulacao, art.no_articulacao,
       ct.co_utilizacao_cartografia, uti.no_utilizacao,
       ct.co_situacao_status, sits.no_situacao_status,
       ct.ds_informacoes_adicionais_metadados,
       ct.co_id_identificador_unico_imovel, ct.ds_colecao_foto_indice,
       ct.ds_colecao_carta_foto_indice, ds_colecao_cod_articulacao,
       ct.ds_titulo_prod_cartografico, ct.ic_colecao_prod_cartografico,
       ct.dt_criacao_identificacao, ct.dt_alteracao_identificacao, ct.dt_publicacao_identificacao,
       ct.dt_criacao_metadado, ct.dt_alteracao_metadado, ct.dt_publicacao_metadado,
       ct.id_colecao, ct.nu_processo_demarcatorio, 
       ct.co_cadastrador,
       ct.ds_contrato_servico, ct.nu_canto_superior_n_esq, ct.nu_canto_superior_e_esq,
       ct.nu_canto_inferior_n_dir, ct.nu_canto_inferior_e_dir,
       ct.ic_template, ct.co_produto_geo, ct.co_tipo_retangulo_envol,
       ct.co_geoidentificacao, ct.co_produto_cdg, pcdg.no_produto_cdg,
       ct.nu_qtd_prod_analogico, ct.ds_ident_cdg_observacao, ct.ds_sis_ref_observacao, ct.ds_ident_observacao,
       pgeo.geom_produto
  from geometadados.tb_produto_cartografico ct  
       left join geometadados.tb_status_produto sp on sp.id_status_produto = ct.co_status_produto
       left join geometadados.tb_tipo_produto tp on tp.id_tipo_produto = ct.co_tipo_produto
       left join geometadados.tb_responsavel rpd on rpd.id_responsavel = ct.co_responsavel_distribuicao
       left join geometadados.tb_responsavel rpi on rpi.id_responsavel = ct.co_responsavel_identificacao
       left join geometadados.tb_responsavel rpm on rpm.id_responsavel = ct.co_responsavel_identificacao
       left join geometadados.tb_responsavel_funcao fd on fd.id_funcao = ct.co_funcao_identificacao
       left join geometadados.tb_responsavel_funcao fi on fi.id_funcao = ct.co_funcao_identificacao
       left join geometadados.tb_responsavel_funcao fim on fim.id_funcao = ct.co_funcao_identificacao
       left join geometadados.tb_projecao pj on pj.id_projecao = ct.co_projecao 
                                            and pj.co_sistema_referencia = ct.co_sistema_referencia
       left join geometadados.tb_sistema_referencia sr on sr.id_sistema_referencia = ct.co_sistema_referencia
       left join geometadados.tb_representacao_espacial re on re.id_representacao_espacial = ct.co_representacao_espacial
       left join geometadados.tb_escala ec on ec.id_escala = ct.co_escala
       left join geometadados.tb_encode ecid on ecid.id_encode = ct.co_encode_id_cdg
       left join geometadados.tb_encode ecidmd on ecidmd.id_encode = ct.co_encode_id_cdg
       left join geometadados.tb_idioma idm on idm.id_idioma = ct.co_idioma_id_cdg
       left join geometadados.tb_idioma idmtd on idmtd.id_idioma = ct.co_idioma_metadado
       left join geometadados.tb_categoria_tematica ctm on ctm.id_categoria_tematica = ct.co_categoria_tematica
       left join geometadados.tb_nivel_hierarquico nh on nh.id_nivel_hierarquico = ct.co_nivel_hierarquico
       left join geometadados.tb_acesso_off_line off on off.id_acesso_off_line = ct.co_acesso_off
       left join geometadados.tb_formato_distribuicao fdt on fdt.id_formato_distribuicao = ct.co_formato_distribuicao
                                                         and fdt.co_produto_cdg = ct.co_produto_cdg
       left join geometadados.tb_protocolo ptc on ptc.id_protocolo = ct.co_protocolos
       left join geometadados.tb_norma_designacao ndes on ndes.id_norma_designacao = ct.co_norma_designacao
       left join geometadados.tb_articulacao art on art.id_articulacao = ct.co_tipo_de_articulacao
       left join geometadados.tb_utilizacao uti on uti.id_utilizacao = ct.co_utilizacao_cartografia
       left join geometadados.tb_situacao_status sits on sits.id_situacao_status = ct.co_situacao_status
       left join geometadados.tb_produto_cdg pcdg on pcdg.id_produto_cdg = ct.co_produto_cdg
       left join geoservicos.tb_produto_cartografico_geo  pgeo on pgeo.id_produto = ct.co_produto_geo</sql>
