    <connection>Servidor</connection>
    <sql>SELECT 'DOMINIAL'::VARCHAR(15) as "Classe" 
     , (case when vr.in_cpf_cgc = '1' then 'PESSOA FÍSICA'
             when vr.in_cpf_cgc = '2' then 'PESSOA JURÍDICA' 
             else '-' end) as "Tipo Responsável"
     /*, (case when vr.in_cpf_cgc = '1' then substring(vr.cpf_cgc,1,3)||'.'||substring(vr.cpf_cgc,4,3)||'.'||substring(vr.cpf_cgc,7,3)||'-'||substring(vr.cpf_cgc,10,2)
             when vr.in_cpf_cgc = '2' then substring(vr.cpf_cgc,1,2)||'.'||substring(vr.cpf_cgc,3,3)||'.'||substring(vr.cpf_cgc,6,3)||'/'||substring(vr.cpf_cgc,9,4)||'-'||substring(cpf_cgc,10,2)
             else '-' end) as "CPF/CNPJ/UG" */

     , (case when vr.in_cpf_cgc = '1' then substring(vr.cpf_cgc,1,3)||'.'||'***'||'.'||substring(vr.cpf_cgc,7,3)||'-'||substring(vr.cpf_cgc,10,2)
             when vr.in_cpf_cgc = '2' then substring(vr.cpf_cgc,1,2)||'.'||'***'||'.'||substring(vr.cpf_cgc,6,3)||'/'||substring(vr.cpf_cgc,9,4)||'-'||substring(cpf_cgc,13,2)
             else '-' end) as "CPF/CNPJ/CÓD. ENTIDADE"


     , coalesce(rp.no_resp,'-') as "Ocupante"
     , r.no_regime_utiliz "REGIME UTILIZAÇÃO"
     --, (case when to_char(u.da_inicio_utiliz,'DD-MM-YYYY') = '1900-01-01' then ''
     --        else to_char(u.da_inicio_utiliz,'DD-MM-YYYY') end)::character varying(10) as "Data do Início da Utilização"
  FROM siapa.siapa_a_imovel i 
	   INNER JOIN siapa.siapa_t_uf uf on i.ed_numero_uf = uf.nu_uf
	   INNER JOIN siapa.siapa_t_municipio mun on i.ed_mun = mun.nu_mun
       INNER JOIN siapa.siapa_t_conceituacao_terreno c on i.co_conceituacao_terreno = c.co_conceituacao_terreno
	   INNER JOIN siapa.siapa_a_utilizacao u 
    	     INNER JOIN siapa.siapa_t_regime_utilizacao r on u.co_regime_utiliz = r.co_regime_utiliz
             INNER JOIN siapa.siapa_a_responsavel rp INNER JOIN siapa.siapa_v_endereco_responsavel vr on vr.nu_resp = rp.nu_resp
             on rp.nu_resp = u.nu_resp
       on i.nu_rip = u.nu_rip and u.co_estado_utiliz = '1'
       LEFT JOIN siapa.siapa_t_tipoimov ti on ti.co_tipo_imov = i.co_classe_imov
 WHERE i.co_situacao_imov = '1'
   AND rp.no_resp is not null
union
SELECT 'USO ESPECIAL'::VARCHAR(15) as "Classe" 
     , 'ENTIDADE DA APF' as "Tipo Responsável"
     , ug.co_ug::varchar(14) as "CPF/CNPJ/CÓD. ENTIDADE"
     , upper(ug.no_ug) as "Ocupante"
     --, r.texto 
     , r.no_regimeutilizacao
     --, to_char(u.uti_da_inicio,'DD-MM-YYYY')::character varying(10) as "Data do Início da Utilização"     
  FROM spiunet.imovel i
       INNER  JOIN spiunet.utilizacao u 
    	     INNER JOIN spiunet.regimeutilizacao r on u.uti_co_regime = r.co_regimeutilizacao
             --INNER JOIN spiunet.tb_regimeutiliz_dadosaberto r on u.uti_co_regime = r.co_regimeutilizacao
    	     INNER JOIN spiunet.v_ug ug on ug.co_ug = u.uti_co_ug
       ON i.imv_nu_rip = u.uti_nu_rip_imovel and u.uti_co_motivocancelamentoutilizacao = '101'

       INNER JOIN spiunet.municipio mun on i.imv_co_municipio = mun.co_municipio

       INNER JOIN spiunet.conceituacaoterreno c on i.imv_co_conceituacaoterreno = c.co_conceituacaoterreno
       LEFT JOIN spiunet.tipoimovel ti on ti.co_tipoimovel = i.imv_co_tipoimovel
 WHERE i.imv_co_motivocancelamentoimovel = '101'
   AND u.uti_co_regime not in (25,26) -- Regimes de Doação não são relacionados pois não pertencem mais a união
ORDER BY 1,4</sql>
