    <sql>SELECT DISTINCT
      'DOMINIAL'::VARCHAR(15) as "Classe" 
     , siapa.fn_calcula_dv_rip(i.nu_rip)::VARCHAR(13) as "Número do RIP"
     , to_char(coalesce(i.da_inclusao_cad_imov,coalesce(da_informacao_imov, da_inicio_utilizacao_efetiva)),'DD-MM-YYYY')::varchar(10) as "Data de cadastramento" 
     , mun.sg_uf::VARCHAR(2) as "UF"
     , RTRIM(UPPER(mun.no_mun)) as "Município"
     , UPPER((ltrim(rtrim(coalesce(i.ed_tipo_logr,'')))||' '||ltrim(rtrim(coalesce(i.ed_logr,'')))||' '||ltrim(rtrim(coalesce(i.ed_numero,'')))||' '||ltrim(rtrim(coalesce(i.ed_complemento,'')))||' CEP: '||ltrim(rtrim(coalesce(i.ed_cep,''))))) as "Endereço"
     , RTRIM(UPPER(i.ed_bairro_distrito)) as "Bairro"
     , RTRIM(UPPER(c.no_conceituacao_terreno)) as "Conceituação"
     , 'TERRENO'::VARCHAR(20) as "Tipo Imóvel"
     , coalesce(r.no_regime_utiliz, 'SEM REGISTRO DE UTILIZAÇÃO')::VARCHAR(30) as "Regime de Utilização"
     , case when r.no_regime_utiliz is not null 
             THEN 'EM USO'::VARCHAR(15) ELSE 'SEM REGISTRO'::VARCHAR(15) end as "Situação da Utilização"
     , 'União (Adm. Pub. Fed. direta)'::VARCHAR(50) as "Proprietário Oficial"
     , r.no_regime_utiliz "REGIME UTILIZAÇÃO"
     , (case when to_char(u.da_inicio_utiliz,'DD-MM-YYYY') = '1900-01-01' then ''
             else to_char(u.da_inicio_utiliz,'DD-MM-YYYY') end)::character varying(10) as "Data do Início da Utilização"
     , to_char(i.mq_area_terreno_total,'999G999G999D99') as "Área do Terreno Total"
     , to_char(i.mq_area_terreno_uniao,'999G999G999D99') as "Área da União"
  FROM siapa.siapa_a_imovel i 
       INNER JOIN siapa.V_municipio mun on i.ed_mun = mun.nu_mun
       LEFT JOIN OLTP.siapa.v_conceituacao_terreno c on i.co_conceituacao_terreno = c.co_conceituacao_terreno
	   LEFT  JOIN siapa.siapa_a_utilizacao u 
    	     INNER JOIN siapa.V_regime_utiliz r on u.co_regime_utiliz = r.co_regime_utiliz
             --INNER JOIN siapa.siapa_a_responsavel rp on rp.nu_resp = u.nu_resp
       on i.nu_rip = u.nu_rip and u.co_estado_utiliz = '1'
       LEFT JOIN OLTP.siapa.v_tipo_imov ti on ti.co_tipo_imov = i.co_classe_imov WHERE i.co_situacao_imov = '1'
union
SELECT case when u.uti_co_regime &lt;&gt; '15' then 'USO ESPECIAL'::varchar(15) else 'IMOVEL FUNCIONAL' end as "Classe" 
     , i.imv_nu_rip::varchar(13) as "Número do RIP"
     , to_char(i.imv_da_cadastro,'DD-MM-YYYY') as "Data de cadastramento" 
     , UPPER(i.imv_sg_uf) as "UF"
     , UPPER(mun.no_municipio) as "Município"
     , UPPER((ltrim(rtrim(coalesce(i.imv_ed_tipologradouro,' ')))||' '||ltrim(rtrim(i.imv_ed_logradouro))||' '||ltrim(rtrim(i.imv_ed_numero))||' '||ltrim(rtrim(coalesce(i.imv_ed_complemento,' ')))||' CEP: '||ltrim(rtrim(i.imv_ed_cep::text)))) as "Endereço"
     , UPPER(i.imv_ed_bairro) as "Bairro"
     , UPPER(c.no_conceituacaoterreno) as "Conceituação"
     , UPPER(ti.no_tipoimovel) as "Tipo Imóvel"
     , coalesce(r.no_regimeutilizacao, '-') as "Regime de Utilização"
     , case when u.uti_co_regime &lt;&gt; 4 then 'EM USO' ELSE 'SEM REGISTRO' end as "Situação da Utilização"
     , po.no_proprietariooficial as "Proprietário Oficial"
     , r.no_regimeutilizacao
     , to_char(u.uti_da_inicio,'DD-MM-YYYY') as "Data do Início da Utilização"
     , to_char(i.imv_mq_areaterreno,'999G999G999D99') as "Área do Terreno Total"
     , to_char(i.imv_mq_areaterreno,'999G999G999D99') as "Área da União"
  FROM spiunet.imovel i
       LEFT  JOIN spiunet.utilizacao u 
    	     INNER JOIN spiunet.regimeutilizacao r on u.uti_co_regime = r.co_regimeutilizacao
       ON i.imv_nu_rip = u.uti_nu_rip_imovel and u.uti_co_motivocancelamentoutilizacao = '101'
       INNER JOIN spiunet.municipio mun on i.imv_co_municipio = mun.co_municipio
       INNER JOIN spiunet.conceituacaoterreno c on i.imv_co_conceituacaoterreno = c.co_conceituacaoterreno
       LEFT JOIN spiunet.tipoimovel ti on ti.co_tipoimovel = i.imv_co_tipoimovel
       INNER JOIN spiunet.proprietariooficial po on po.co_proprietariooficial = i.imv_co_proprietariooficial
 WHERE i.imv_co_motivocancelamentoimovel = '101'
	
       </sql>
